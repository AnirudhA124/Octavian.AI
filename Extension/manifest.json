{
  "manifest_version": 3,
  "name": "Octavian Document Chat",
  "version": "1.0",
  "description": "Chat with your documents",
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Open Octavian"
  }
}
